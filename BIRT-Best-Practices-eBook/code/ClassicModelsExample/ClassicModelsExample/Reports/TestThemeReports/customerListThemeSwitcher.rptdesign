    <property name="comments">This report is an example of Theme switching at runtime.  When running, there is a parameter to indicate what theme color you want to use.  In the initialize method of the report there is code that will change the Theme at runtime.</property>
    <html-property name="description">*****************************************************************&#13;
**  This Report file is part of the Classic Models example        **&#13;
**  provided with the eBook:                                                        **&#13;
**  Le BIRT Expert's Guide to BIRT Best Practices                       **&#13;
**                                                                                                      **&#13;
**  Visit http://www.lebirtexpert.com for more information   **&#13;
**                                                                                                      **&#13;
*****************************************************************</html-property>
    <method name="initialize"><![CDATA[

var pThemeColor = reportContext.getParameterValue("pThemeColor");
var newThemeName = "reportThemes.predefinedReportThemeGray";  // default

if(pThemeColor == "orange") {
	newThemeName = "reportThemes.predefinedReportThemeOrange";
} else if(pThemeColor == "red") {
	newThemeName = "reportThemes.predefinedReportThemeRed";
} else if(pThemeColor == "green") {
	newThemeName = "reportThemes.predefinedReportThemeGreen";
} else if(pThemeColor == "purple") {
	newThemeName = "reportThemes.predefinedReportThemePurple";
} else if(pThemeColor == "teal") {
	newThemeName = "reportThemes.predefinedReportThemeTeal";
} else if(pThemeColor == "copper") {
	newThemeName = "reportThemes.predefinedReportThemeCopper";
} else if(pThemeColor == "darkBlue") {
	newThemeName = "reportThemes.predefinedReportThemeDarkBlue";
} else if(pThemeColor == "mediumBlue") {
	newThemeName = "reportThemes.predefinedReportThemeMediumBlue";
} else if(pThemeColor == "gray") {
	newThemeName = "reportThemes.predefinedReportThemeGray";
} 

try {
	reportContext.getDesignHandle().setThemeName(newThemeName);
} catch(err) {
	logMessageInfo("customerListThemeSwitcher:beforeRender():error in theme switching: " + err.message);
}]]></method>
